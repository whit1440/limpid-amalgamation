{
  "api": {
    "protocol": "https",
    "host": "www.issue-tracker-host.com",
    "path": "/project-or-corp-or-whatever",
    "authToken": "basic auth token",
    "headers": {
      "Accept": "application/json",
      "Content-Type": "application/json",
      "Origin": "*"
    },
    "noPost": true,
    "crossOrigin": true
  },
  "scm": {
    "path": ".",
    "story": "[a-zA-Z][- ][0-9]{5}",
    "merge": "Merge branch 'feature/([a-zA-Z][- ][0-9]{5}).*' into '([a-zA-Z]*)'",
    "feature": "feature/[a-zA-Z][- ][0-9]{5}",
    "release": "release/[a-zA-Z][- ][0-9]{5}"
  },
  "logger": {
    "level": 0
  },
  "deploy": {
    "tiers": ["local", "merged", "Dev", "QA", "Stage", "Prod"],
    "sprintSchedule": "My Sprint Schedule",
    "sprintName": "Sprint 6"
  }
}
